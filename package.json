    "gh-pages": "^2.0.1",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
